<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cn.car.dao.StudentDao" >
	<select id="selectByFy" resultType="com.cn.car.entity.Student" parameterType="Map">
		SELECT *
		FROM student
		<if test="a!=null">
			<where> 
				<if test="name!=null and name!=''"> name=#{name}</if>
				<if test="age!=null and age!=''">AND  age=#{age}</if>
			</where>
			LIMIT #{a},#{b}
		</if>
	</select>
	
		<select id="findUserList" resultType="com.cn.car.entity.Student" parameterType="Map">
		SELECT *
		FROM student
	</select>
	
	
	
</mapper>